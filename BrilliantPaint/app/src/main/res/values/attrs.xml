<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CoolMenuFrameLayout">
        <attr name="num" format="enum">
            <enum name="one" value="1" />
            <enum name="two" value="2" />
            <enum name="three" value="3" />
            <enum name="four" value="4" />
            <enum name="five" value="5" />
        </attr>
        <attr name="titleSize" format="dimension"/>
        <attr name="titleColor" format="color"/>
        <attr name="titleIcon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="RoundAngleImageView">  
        <attr name="roundWidth" format="dimension" />  
        <attr name="roundHeight" format="dimension" />  
  </declare-styleable>  


    <declare-styleable name="ShadowImageView">
        <attr name="shadowRound" format="dimension" />
        <attr name="shadowSrc" format="reference" />
        <attr name="shadowColor" format="color" />
    </declare-styleable>

</resources>